
<%= image_tag("reftrkr.png", :alt => "rss feed") %>
<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "reftrkr", '#', id: "logo" %> <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Home", '#' %></li>
        <li><%= link_to "sign_up", '#' %></li>
        <li><%= link_to "success", '#success' %></li>
        <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-primary" %>
      </ul> </nav>
    </div> </header>

    <table>
      <thead>
        <style>
          table, th, td {
            padding: 10px;
            border: 1px solid black;
            border-collapse: collapse;
            text-align: center;
          }
        </style>
        <th>Email</th>
        <th>Counter</th>
        <th colspan="3">User Id</th>
      </thead>
      <tbody>
        <% @user.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.counter %></td>
          <td><%= user.id %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
