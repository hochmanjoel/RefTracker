<h1>Email Database</h1>

<table>
  <thead>
    <th>Email</th>
    <th>Counter</th>
    <th colspan="3"></th>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.counter %></td>
        <td><%= user.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
