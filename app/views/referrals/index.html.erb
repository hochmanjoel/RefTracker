
<%= image_tag("reftrkr.png", :alt => "rss feed") %>
<table>
  <thead>
    <style>
      table, th, td {
        padding: 10px;
        border: 1px solid black;
        border-collapse: collapse;
        text-align: center;
      }
    </style>
    <th>Email</th>
    <th>Counter</th>
    <th colspan="3">User Id</th>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.counter %></td>
        <td><%= user.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
